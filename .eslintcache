ext-white text-xl justify-between p-4 items-center\">\n                <Link to=\"/\" className=\"border border-blue-800 focus:border-white p-2\">\n                    Home\n                </Link>\n                <form className=\"flex flex-row items-center border rounded border-white\" onSubmit={this.handleSearch}>\n                    <button type=\"submit\" className=\"mx-2 focus:outline-none\">\n                        <svg\n                            xmlns=\"http://www.w3.org/2000/svg\"\n                            className=\"w-6 h-6\"\n                            fill=\"none\"\n                            stroke=\"currentColor\"\n                            viewBox=\"0 0 24 24\"\n                        >\n                            <path\n                                stroke-linecap=\"round\"\n                                stroke-linejoin=\"round\"\n                                stroke-width=\"2\"\n                                d=\"M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z\"\n                            />\n                        </svg>\n                    </button>\n\n                    <label htmlFor=\"keywords\">\n                        <input\n                            id=\"keywords\"\n                            name=\"keywords\"\n                            type=\"text\"\n                            onChange={this.handleInput}\n                            placeholder=\"Search for products\"\n                            className=\"p-2 text-sm text-gray-700 focus:outline-none flex flex-grow\"\n                        />\n                    </label>\n                </form>\n                <div className=\"flex flex-row justify-between items-center gap-x-10\">\n                    {user._id == \"\" ?\n                        <Link to=\"/login\" className=\"border border-blue-800 focus:border-white p-2\">\n                            Login\n                        </Link> : <p className=\"text text-sm\">Welcome {user.firstName}!!</p>}\n                    {user._id == \"\" ?\n                        <Link to=\"/register\" className=\"border border-blue-800 focus:border-white p-2\">\n                            Register\n                        </Link> : <button onClick={this.handleLogOut}\n                            className=\"border border-blue-800 focus:border-white p-2\">Logout</button>}\n                    <Link to=\"/cart\" className=\"border border-blue-800 focus:border-white p-2\">\n                        <svg\n                            className=\"focus:outline-none h-6 w-6 text-white\"\n                            xmlns=\"http://www.w3.org/2000/svg\"\n                            fill=\"none\"\n                            stroke=\"currentColor\"\n                            viewBox=\"0 0 24 24\"\n                        >\n                            <path\n                                stroke-linecap=\"round\"\n                                stroke-linejoin=\"round\"\n                                stroke-width=\"2\"\n                                d=\"M3 3h2l.4 2M7 13h10l4-8H5.4M7 13L5.4 5M7 13l-2.293 2.293c-.63.63-.184 1.707.707 1.707H17m0 0a2 2 0 100 4 2 2 0 000-4zm-8 2a2 2 0 11-4 0 2 2 0 014 0z\"\n                            />\n                        </svg>\n                    </Link>\n                </div>\n            </div>\n        );\n    }\n}\n\ninterface StateProps {\n    user: IUser;\n}\n\nconst mapStateToProps = (state: AppState): StateProps => ({\n    user: state.user.user,\n});\n\n\nconst mapDispatchToProps = (dispatch: any): { actions: Actions } => ({\n    actions: {\n        searchProducts: (keywords: string) => dispatch(searchProducts(keywords)),\n        logOutUser: () => dispatch(logOutUser()),\n    },\n});\nexport default connect(mapStateToProps, mapDispatchToProps)(Header);\n","/Users/anuppoudel/shopping-cart-app/src/components/Body/index.tsx",[],"/Users/anuppoudel/shopping-cart-app/src/ActionTypes.ts",[],"/Users/anuppoudel/shopping-cart-app/src/com