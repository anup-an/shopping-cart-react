te = {\n            products: data.products,\n            count: data.products.length,\n            size: '',\n            sort: '',\n            cartItems: JSON.parse(localStorage.getItem('cartItems') || '[]'),\n        };\n    }\n\n    sortByPrice = (event: React.ChangeEvent<HTMLSelectElement>): void => {\n        this.setState({ sort: event.target.value });\n        if (event.target.value === 'Lowest') {\n            this.setState({\n                sort: event.target.value,\n                products: data.products.slice().sort((a, b) => (a.price > b.price ? 1 : -1)),\n            });\n        } else if (event.target.value === 'Highest') {\n            this.setState({\n                sort: event.target.value,\n                products: data.products.slice().sort((a, b) => (a.price < b.price ? 1 : -1)),\n            });\n        } else if (event.target.value === 'Newest') {\n            this.setState({\n                sort: event.target.value,\n                products: data.products.slice().sort((a, b) => (a.id > b.id ? 1 : -1)),\n            });\n        }\n    };\n\n    filterBySize = (event: React.ChangeEvent<HTMLSelectElement>): void => {\n        if (event.target.value === 'ALL') {\n            this.setState({\n                size: event.target.value,\n                products: data.products,\n            });\n        } else {\n            this.setState({\n                size: event.target.value,\n                products: data.products.filter((product) => product.availableSizes.indexOf(event.target.value) >= 0),\n            });\n        }\n    };\n\n    addToCart = (product: IProduct): void => {\n        const { cartItems } = this.state;\n        if (cartItems.length === 0) {\n            cartItems.push({ ...product, count: 1 });\n        } else if (cartItems.length !== 0) {\n            const cart = cartItems.find((item) => item.id === product.id);\n            if (cart && cart.count) {\n                cartItems[cartItems.indexOf(cart)].count = cart.count + 1;\n            } else if (!cart) {\n                cartItems.push({ ...product, count: 1 });\n            }\n        }\n\n        this.setState({ cartItems });\n        localStorage.setItem('cartItems', JSON.stringify(cartItems));\n    };\n\n    removeFromCart = (item: ICart): void => {\n        const { cartItems } = this.state;\n        this.setState({ cartItems: cartItems.filter((product) => product.id !== item.id) });\n        localStorage.setItem('cartItems', JSON.stringify(cartItems.filter((product) => product.id !== item.id)));\n    };\n\n    createOrder = (order: IOrder): void => {\n        console.log(order);\n    };\n\n    render(): JSX.Element {\n        const { products, count, sort, size, cartItems } = this.state;\n\n        return (\n            <div className=\"flex flex-col\">\n                <div>\n                    <Header />\n                </div>\n                <div>\n                    <Body\n                        products={products}\n                        count={count}\n                        sort={sort}\n                        size={size}\n                        sortByPrice={this.sortByPrice}\n                        filterBySize={this.filterBySize}\n                        addToCart={this.addToCart}\n                        removeFromCart={this.removeFromCart}\n                        createOrder={this.createOrder}\n                        cartItems={cartItems}\n                    />\n                </div>\n                {/* <div>\n                    <Footer />\n                </div> */}\n            </div>\n        );\n    }\n}\nexport default App;\n","/Users/anuppoudel/shopping-cart-app/src/components/Body/index.tsx",[],"/Users/anuppoudel/shopping-cart-app/src/components/Header/index.tsx",[],"/Users/anuppoudel/shopping-cart-app/src/components/Footer/index.tsx",[],["51","52","53","54","55"],"/Users/anuppoudel/shopping-cart-app/src/components/Body/Products.tsx",[],"/Users/anuppoudel/shopping-cart-app/src/component